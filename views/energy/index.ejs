<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" 
      content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Energyy</title>
</head>
<body>
    <h1>Energy Booster</h1>
    <ul>
      <% energy.forEach(function(t) { %>
        <li>
          <%= t.energy %>
            - 
          <%= t.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
</body>
</html>
